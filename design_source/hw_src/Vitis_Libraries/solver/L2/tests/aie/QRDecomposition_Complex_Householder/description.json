{
    "name": "QR decomposition With Householder", 
    "description": [
        "QRD based on Householder Algorithm"
    ],
    "flow": "aie",
    "keywords": [
        "aiecompiler", 
        "aiesimulator",
        "stream_connect"
    ], 
    "key_concepts": [
        "AI Engine", 
        "Adaptive Data Flow", 
        "Graph"
    ],
    "platform_allowlist": [
        "vck190"
    ],
    "platform_blocklist": [

    ],
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ],
    "pre_build" : [
        {
            "build_cmd": "cd tv && make tv -f tv.mk"
        }
    ],
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "./aie",
                "LIB_DIR/L1/include/aie",
                "LIB_DIR/L2/include/aie",
                "LIB_DIR/L1/src/aie"
            ]
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "./aie/aie_graph.cpp",
                    "pl-freq": 1000,
                    "workdir": "./Work",
                    "name": "libadf.a",
                    "aie_kernel_files":["./aie/aie_adder.cc"]
                }
            ]
        }
    ],
    "match_makefile" : "true",
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_aie_sim",
            "vitis_aie_x86sim"
        ],
        "category": "canary"
    }
}
